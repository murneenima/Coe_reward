            });
        });


        var dataHouse = "{{dataHouse}}"
        var data_Larry = JSON.parse(decodeURI(dataHouse))
        var House_Larry = [];
        //console.log(data_Larry)
        let display_MemberAll = "<thead><th>No</th><th>Member ID</th><th>Name</th><th>Last Name</th><th>Total Point</th><th>Available Point</th></thead>";

        //console.log(data_Larry.length)
        for (let i = 0; i < data_Larry.length; i++) {
            House_Larry.push(data_Larry[i])
            myFunction()
            //display_MemberAll += "<tbody><tr><td>" + (i + 1) + "</td><td>" + Member[i].Member_ID + "</td><td>" + Member[i].Member_Name + "</td><td>" + Member[i].Member_Lastname + "</td><td>" + Member[i].Member_House + "</td></td><td><p data-placement='top' data-toggle='tooltip' title='Edit'><button class='btn btn-primary btn-xs'  onclick='ShowModal_Edit(" + i + ")' data-title='Edit' data-toggle='modal' data-target='#edit' ><i class='fas fa-edit'></i></button></p></td><td><p data-placement='top' data-toggle='tooltip' title='Delete'><button onclick='ShowModal_delete(" + i + ")' class='btn btn-danger btn-xs' data-title='Delete' data-toggle='modal'><i class='fas fa-trash-alt'></i></button></p></td></tr></tbody>"
        }

        for (let i = 0; i < House_Larry.length; i++) {
            display_MemberAll += "<tbody><tr><td>" + (i + 1) + "</td><td>" + House_Larry[i].Member_ID + "</td><td>" + House_Larry[i].Member_Name + "</td><td>" + House_Larry[i].Member_Lastname + "</td><td>"+"+"+House_Larry[i].Member_Total+"</td><td>"+"+"+House_Larry[i].Member_Available+"</td></tr></tbody>"
        }

        document.getElementById("member_larry").innerHTML = display_MemberAll

        function myFunction() {
            House_Larry.sort(function (a, b) { return a.Member_Total - b.Member_Total });
            //console.log(Member)
        }
        

